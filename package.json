{
  "name": "fairfield-airport-cars",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && vercel --prod",
    "deploy:preview": "npm run build && vercel",
    "init-cms": "node scripts/init-cms.js",
    "migrate-content": "node scripts/migrate-content-to-cms.js",
    "add-missing-content": "node scripts/add-missing-content.js",
    "fix-cms": "node scripts/fix-cms-permissions.js",
    "test": "npm run test:integration && npm run test:unit && npm run test:e2e",
    "test:integration": "vitest run tests/integration --coverage",
    "test:unit": "NODE_OPTIONS=\"--max-old-space-size=4096\" vitest run tests/unit/essential-tests.spec.tsx tests/unit/core-functionality-test.spec.tsx tests/unit/basic-component-test.spec.tsx tests/unit/msw-test.spec.ts --coverage",
    "test:e2e": "playwright test --config config/playwright.config.ts tests/e2e/critical-flows/streamlined-user-flows.spec.ts",
    "test:e2e:pages": "playwright test --config config/playwright.config.ts tests/e2e/page-loading-test.spec.ts",
    "test:msw": "vitest run tests/unit/msw-test.spec.ts",
    "vitest": "vitest",
    "vitest:ui": "vitest --ui",
    "vitest:run": "vitest run",
    "vitest:coverage": "vitest run --coverage",
    "firebase:rules:deploy": "./scripts/firebase-rules-sync.sh deploy",
    "firebase:rules:pull": "./scripts/firebase-rules-sync.sh pull",
    "firebase:rules:sync": "./scripts/firebase-rules-sync.sh sync",
    "firebase:rules:validate": "./scripts/firebase-rules-sync.sh validate",
    "firebase:rules:diff": "./scripts/firebase-rules-sync.sh diff",
    "firebase:rules:status": "./scripts/firebase-rules-sync.sh status",
    "test:user:setup": "node scripts/setup-test-user.js setup",
    "test:user:cleanup": "node scripts/setup-test-user.js cleanup",
    "test:e2e:with-auth": "npm run test:user:setup && npm run test:e2e:pages && npm run test:user:cleanup",
    "test:e2e:auth": "playwright test --config config/playwright.config.ts tests/e2e/authenticated-page-test.spec.ts",
    "test:e2e:simple-auth": "playwright test --config config/playwright.config.ts tests/e2e/simple-auth-test.spec.ts"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/react": "^6.1.10",
    "@googlemaps/google-maps-services-js": "^3.4.1",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@stripe/stripe-js": "^7.4.0",
    "@types/dompurify": "^3.0.5",
    "@types/styled-components": "^5.1.34",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "dompurify": "^3.2.6",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "ics": "^2.41.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "nodemailer": "^6.9.8",
    "openai": "^5.9.0",
    "puppeteer": "^24.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.59.0",
    "square": "^43.0.0",
    "stripe": "^18.3.0",
    "styled-components": "^6.1.19",
    "twilio": "^5.7.2",
    "uuid": "^11.1.0",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.7",
    "@types/nodemailer": "^6.4.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "glob": "^11.0.3",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "node-fetch": "^3.3.2",
    "node-mocks-http": "^1.17.2",
    "supertest": "^7.1.3",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "whatwg-fetch": "^3.6.20"
  }
}
