{
  "name": "fairfield-airport-cars",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:safe": "./scripts/dev-server.sh",
    "dev:clean": "./scripts/dev-server.sh --clean",
    "dev:status": "./scripts/check-dev-status.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && vercel --prod",
    "deploy:preview": "npm run build && vercel",
    "init-cms": "node scripts/init-cms.js",
    "test": "npm run test:integration && npm run test:unit && npm run test:e2e",
    "test:integration": "jest --testPathPattern=tests/integration --coverage",
    "test:unit": "jest --testPathPattern=tests/unit --coverage",
    "test:e2e": "playwright test tests/e2e/critical-flows/streamlined-user-flows.spec.ts",
    "test:page-integrity": "playwright test tests/e2e/page-integrity.test.ts",
    "pre-commit": "./scripts/pre-commit-check.sh",
    "test:user-behavior": "jest --testPathPattern=tests/integration/user-behavior",
    "test:component-integration": "jest --testPathPattern=tests/integration/component-integration",
    "test:user-journeys": "jest --testPathPattern=tests/integration/user-journeys",
    "test:comprehensive": "node scripts/test-suite.js",
    "test:analytics": "node scripts/test-analytics.js",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:coverage": "jest --coverage",
    "test:smoke": "jest --testPathPattern=tests/smoke --passWithNoTests",
    "test:build": "jest --testPathPattern=tests/smoke/build-verification --passWithNoTests",
    "test:health": "jest --testPathPattern=tests/smoke/health-check --passWithNoTests",
    "analyze": "node scripts/daily-analysis.js",
    "analyze:setup": "chmod +x scripts/setup-cron.sh && ./scripts/setup-cron.sh",
    "analyze:run": "node scripts/daily-analysis.js",
    "analyze:reports": "ls -la reports/",
    "analyze:logs": "tail -f logs/daily-analysis.log",
    "health-check": "node scripts/health-check.js",
    "backup": "node scripts/backup-data.js",
    "monitor": "node scripts/monitor-app.js"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/react": "^6.1.10",
    "@googlemaps/google-maps-services-js": "^3.4.1",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@stripe/stripe-js": "^7.4.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/dompurify": "^3.0.5",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.6",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "ics": "^2.41.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "nodemailer": "^6.9.8",
    "openai": "^5.9.0",
    "puppeteer": "^24.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.59.0",
    "square": "^43.0.0",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "twilio": "^5.7.2",
    "uuid": "^11.1.0",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.7",
    "@types/nodemailer": "^6.4.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.10.4",
    "node-mocks-http": "^1.17.2",
    "supertest": "^7.1.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.20"
  }
}
